# Production Environment Configuration
# Copy to .env and fill in actual values

NODE_ENV=production
PORT=4000

# Database - Use managed PostgreSQL service
DATABASE_URL="postgresql://username:password@production-host:5432/medicaid_ce"

# JWT Secret - MUST be secure in production
# Generate with: openssl rand -base64 64
JWT_SECRET=""  # REQUIRED: Fill this in!
JWT_EXPIRES_IN="8h"

# CORS - Your actual frontend URL
ALLOWED_ORIGINS="https://your-demo-app.com"

# Logging
LOG_LEVEL="info"
